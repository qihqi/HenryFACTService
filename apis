GET /pedido/<id>
POST /pedido

GET /nota/<id>
POST /nota
    options:
        increment codigo
        update/create prod
        update/create client
PUT /nota/<id> #commit a nota
DELETE /nota/<id>

# lista de precio
GET /alm/<almacen_id>/producto/<prod_id>
PUT /alm/<almacen_id>/producto/<prod_id>
DELETE /alm/<almacen_id>/producto/<prod_id>
POST /alm/<almacen_id>/producto/<prod_id>

GET /producto/:id
PUT /producto/:id #crear producto 
POST /producto/:id #cambiar precio etc
DELETE /producto/:id #eliminar

GET /cliente/<id>
POST /cliente/<id>
PUT /cliente/<id>
DELETE /cliente/<id>

